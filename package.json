{
  "name": "react-loading-image-effect",
  "version": "0.6.0",
  "description": "Progressive image loading for React",
  "main": "dist.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "build": "babel src/index.js --out-file dist.js",
    "build_wp": "npm run lint && npm run build-lib && webpack --mode production",
    "build-lib-watch": "tsc -w",
    "build-lib": "tsc && npm run copy-css-to-lib && npm run copy-svg-to-lib && npm run copy-png-to-lib && npm run copy-woff2-to-lib",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject",
    "copy-css-to-lib": "cpx \"./src/**/*.css\" ./build/lib",
    "copy-woff2-to-lib": "cpx \"./src/**/*.woff2\" ./build/lib",
    "copy-svg-to-lib": "cpx \"./src/**/*.svg\" ./build/lib",
    "copy-png-to-lib": "cpx \"./src/**/*.png\" ./build/lib",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "start": "webpack-dev-server --mode development --open --hot",
    "umd": "webpack --config webpack.config.umd.js",
    "demo": "webpack-dev-server --inline --hot --config demo/webpack.config.js --content-base demo/",
    "pretty": "prettier --write '{tests,src}/*.{ts,tsx}'",
    "check-types": "tsc"
  },
  "keywords": [
    "react",
    "effect",
    "images",
    "loading"
  ],
  "repository": "https://github.com/trkhanh/react-loading-image-effect.git",
  "author": "KhanhTran <trkhanh8@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^25.1.0",
    "prettier": "^1.14.2",
    "raf": "^3.4.0",
    "react": "^16.12.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.12.0",
    "react-scripts-ts": "1.0.6",
    "react-test-renderer": "^16.5.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0",
    "react-dom": "^15.0.0-0 || ^16.0.0-0"
  },
  "sideEffects": false,
  "dependencies": {
    "@types/enzyme": "^2.7.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^7.0.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^4.4.36",
    "enzyme": "^2.7.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
